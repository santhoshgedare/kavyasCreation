@page
@model kavyasCreation.Areas.Identity.Pages.Account.LogoutModel
@{
    ViewData["Title"] = "Log out";
    Layout = "/Views/Shared/_AuthLayout.cshtml";
}
<h1 class="h3 fw-semibold">Log out</h1>
<p class="text-muted">Are you sure you want to sign out?</p>
<form method="post" class="d-flex gap-2">
    <button class="btn btn-danger" type="submit">Confirm Logout</button>
    <a class="btn btn-outline-secondary" asp-page="/Index" asp-area="">Cancel</a>
</form>
